        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.573" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="127dp"
        android:layout_height="38dp"
        android:fontFamily="sans-serif-condensed"
